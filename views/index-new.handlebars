<!DOCTTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{{title}}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        {{#if brand}}
            <link rel="shortcut icon" href="/images/{{#brand}}{{slug}}{{/brand}}/favicon.ico" type="image/x-icon">
            <link rel="icon" href="/images/{{#brand}}{{slug}}{{/brand}}/favicon.ico" type="image/x-icon">
        {{else}}
            <link rel="shortcut icon" href="/images/mainsite/favicon.ico" type="image/x-icon">
            <link rel="icon" href="/images/mainsite/favicon.ico" type="image/x-icon">
        {{/if}}
        {{> css }}
        {{#if sourceDecoration}}
            {{> sourceDecoration }}
        {{/if}}
    </head>
    <body>
    {{> body}}
    </body>
    {{> script}}
</html>
